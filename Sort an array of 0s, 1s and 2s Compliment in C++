class Solution
{
    public:
   void sort012(int arr[], int n)
    {
        // code here
        int i=0,j=0,k=n-1;
        while(i<=k) {
            if(arr[i]==0) {
                swap(arr[i++],arr[j++]);
            }
            else if(arr[i]==1) {
                i++;
            }
            else if(arr[i]==2) {
                swap(arr[i],arr[k--]);
            }
      
